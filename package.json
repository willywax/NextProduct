{
  "name": "nextproduct",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./build && babel backend -d build",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage",
    "coveralls-coverage": "coveralls < lcov.info",
    "db-migrate": "sequelize db:migrate --env production",
    "db-migrate:dev": "sequelize db:migrate --env development",
    "db-migrate:test": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "dev": "nodemon --exec babel-node ./backend/app.js",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "start": "npm run build && node ./build/app.js",
    "test": "export NODE_ENV=test && npm run db-migrate:test && nyc mocha --require @babel/register ./backend/test/*.js --timeout 60000 --exit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.0",
    "@babel/register": "^7.5.5",
    "@hapi/joi": "^15.1.1",
    "bcrypt": "^3.0.6",
    "cloudinary": "^1.14.0",
    "config": "^3.2.2",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.13.1",
    "sequelize-cli": "^5.5.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.2.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  }
}
